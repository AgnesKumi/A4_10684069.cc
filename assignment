#include<iostream>
using namespace std;
int gcd(int a, int b){

if(b!=0)
	return gcd( a, a%b);
	else
	return a;
}

 int main()
{
	int a,b;
    cout<<"please enter a number "<<endl;
    
	cin>>a;
	cout<<"please enter a second number "<<endl;
	cin>>b;
	cout<<"the greatest common factor of "<< a << " and "<< b << " is: "<<endl;
	cout<<gcd(a,b);
	return 0;
}
